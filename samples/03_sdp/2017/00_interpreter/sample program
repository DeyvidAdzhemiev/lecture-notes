start

    printline 0 end

	assign m  start
				
				assign x + 2 3

				assign y if x then assign x + x 1 
				              else 0

				assign q assign z + x y     

				+ z q

			end
    
    printline m + m 1 z end

    loop k from 0 to 10 step 1 do printline k end

	define fact i j k m do + i + j + k m

	define recprint i do if i then 
	   						    start 
	   						          printline i end 
	   						          call recprint - i 1 .
	   						          
	   						     end
	   						   else  printline i end

	call recprint 10 .


	    

end